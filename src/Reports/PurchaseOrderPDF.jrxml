	<subDataset name="Table Dataset 1" uuid="92dde620-4dc4-4629-b0b4-0126c62c832d"/>
	<subDataset name="purchaseOrderDetails" uuid="9633422d-ac05-47b4-a112-29815cc07d32"/>
	<queryString language="SQL">
		<![CDATA[SELECT
     contact.`name` AS contact_name,
     contact.`phone` AS contact_phone,
     job.`name` AS job_name,
     job.`address` AS job_address,
     job.`city` AS job_city,
     job.`state` AS job_state,
     job.`zip` AS job_zip,
     job.`bidamount` AS job_bidamount,
     job.`status` AS job_status,
     product.`description` AS product_description,
     product.`unitMeasure` AS product_unitMeasure,
     product.`manufacturer` AS product_manufacturer,
     product.`supplier` AS product_supplier,
     product.`price` AS product_price,
     product.`part_id` AS product_part_id,
     productdescription.`productDescription` AS productdescription_productDescription,
     productdescription.`productsize` AS productdescription_productsize,
     purchaseorder.`supplier` AS purchaseorder_supplier,
     purchaseorder.`job` AS purchaseorder_job,
     purchaseorder.`expectedby` AS purchaseorder_expectedby,
     purchaseorder.`contact` AS purchaseorder_contact,
     purchaseorder.`quote` AS purchaseorder_quote,
     purchaseorder.`quotedate` AS purchaseorder_quotedate,
     purchaseorder.`tax` AS purchaseorder_tax,
     purchaseorder.`total` AS purchaseorder_total,
     purchaseorder.`createdby` AS purchaseorder_createdby,
     purchaseorder.`created` AS purchaseorder_created,
     purchaseorder.`shipto` AS purchaseorder_shipto,
     purchaseorder.`status` AS purchaseorder_status,
     purchaseorder.`comments` AS purchaseorder_comments,
     purchaseorderdetails.`product` AS purchaseorderdetails_product,
     purchaseorderdetails.`cost` AS purchaseorderdetails_cost,
     purchaseorderdetails.`orderqty` AS purchaseorderdetails_orderqty,
     purchaseorderdetails.`receivedqty` AS purchaseorderdetails_receivedqty,
     purchaseorderdetails.`tax` AS purchaseorderdetails_tax,
     purchaseorderdetails.`total` AS purchaseorderdetails_total,
     supplier.`companyname` AS supplier_companyname,
     supplier.`contact` AS supplier_contact,
     supplier.`address1` AS supplier_address1,
     supplier.`city` AS supplier_city,
     supplier.`state` AS supplier_state,
     supplier.`postalcode` AS supplier_postalcode,
     supplier.`phone` AS supplier_phone,
     supplier.`fax` AS supplier_fax,
     supplier.`terms` AS supplier_terms,
     supplier.`comments` AS supplier_comments,
     user.`username` AS user_username,
     user.`name` AS user_name,
     user.`password` AS user_password,
     purchaseorderdetails.`orderid` AS purchaseorderdetails_orderid,
     purchaseorder.`orderid` AS purchaseorder_orderid
FROM
     `productdescription` productdescription INNER JOIN `product` product ON productdescription.`pdescID` = product.`description`
     INNER JOIN `purchaseorderdetails` purchaseorderdetails ON product.`id` = purchaseorderdetails.`product`
     INNER JOIN `supplier` supplier ON product.`supplier` = supplier.`supplierid`
     INNER JOIN `purchaseorder` purchaseorder ON supplier.`supplierid` = purchaseorder.`supplier`
     AND purchaseorder.`orderid` = purchaseorderdetails.`orderid`
     INNER JOIN `contact` contact ON purchaseorder.`contact` = contact.`contactid`
     INNER JOIN `job` job ON purchaseorder.`job` = job.`jobid`
     INNER JOIN `user` user ON purchaseorder.`createdby` = user.`userid`]]>
	</queryString>
